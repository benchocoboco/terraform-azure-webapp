		<add key="SmtpDownload" value="webmaster@proverbes-grandmere.net"/>
		<add key="FileToDownload" value="Proverbes.zip"/>
		<add key="TestReq" value="SELECT * FROM sysobjects"/>
		<add key="Redirect" value="no"/> 		

		<!-- Config de diffusion -->
		<add key="WebServicePrefix" value="http://localhost/WebApp"/>
		<add key="URLDownload" value="http://localhost/WebApp/Download.aspx?request="/>
		<add key="URLDetail" value="http://localhost/WebApp/ListProverbe.aspx?id={0}"/>
		<add key="ProverbePath" value="c:\\Proverbes\\WebApp\\XML\\Proverbes.xml"/>
		<add key="ConnStr" value="data source=DATASOURCE;Connect Timeout=30;User ID=USER;Password=PASSWORD;database=DATABASE"/>
